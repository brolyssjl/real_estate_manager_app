<%= form_for @project do |f| %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :country %>
    <%= f.text_field :country %>
  </p>
  <p>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </p>
  <p>
    <%= f.label :neighborhood %>
    <%= f.text_field :neighborhood %>
  </p>
  <p>
    <%= f.label :address %>
    <%= f.text_field :address %>
  </p>
  <p>
    <%= f.label :phone_number %>
    <%= f.text_field :phone_number %>
  </p>
  <p>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :stratum  %>
    <%= f.text_field :stratum %>
  </p>
  <p>
    <%= f.label :activated %>
    <%= f.check_box :activated %>
  </p>
  <p>
    <%= f.label :sales_room_address %>
    <%= f.text_field :sales_room_address %>
  </p>
  <p>
    <%= f.label :sales_room_phone %>
    <%= f.text_field :sales_room_phone %>
  </p>
  <p>
    <%= f.label :sales_room_email %>
    <%= f.text_field :sales_room_email %>
  </p>
  <p>
    <%= f.label :estimated_start_date %>
    <%= f.date_field :estimated_start_date %>
  </p>
  <p>
    <%= f.label :estimated_end_date %>
    <%= f.date_field :estimated_end_date %>
  </p>
  <%= f.submit %>
<% end %>